{
  "name": "webpack-react-template",
  "version": "1.0.0-alpha.0",
  "private": true,
  "description": "Webpack 5 template for React Typescript projects.",
  "license": "MIT",
  "repository": "git@github.com:alex-kim-dev/webpack-react-template.git",
  "author": "Alex Kim (https://github.com/alex-kim-dev)",
  "homepage": "https://alex-kim-dev.github.io/webpack-react-template#readme",
  "bugs": "https://github.com/alex-kim-dev/webpack-react-template/issues",
  "scripts": {
    "analyze": "npm run build -- --analyze",
    "build": "cross-env NODE_ENV=production webpack",
    "format": "prettier --check --ignore-path .gitignore \"**/*.{html,json,yml,md}\"",
    "lint": "eslint --cache --ignore-path .gitignore --ext .ts,.tsx .",
    "prepare": "husky install",
    "serve": "webpack serve --history-api-fallback --static-directory ./dist",
    "start": "webpack serve --hot",
    "test": "echo \"Error: no test specified\" && exit 1",
    "typecheck": "tsc",
    "update-caniuse": "npx browserslist --update-db"
  },
  "dependencies": {
    "react": "18.1.0",
    "react-dom": "18.1.0"
  },
  "devDependencies": {
    "@babel/core": "7.17.9",
    "@babel/preset-env": "7.16.11",
    "@babel/preset-react": "7.16.7",
    "@babel/preset-typescript": "7.16.7",
    "@commitlint/cli": "16.2.4",
    "@commitlint/config-conventional": "16.2.4",
    "@pmmmwh/react-refresh-webpack-plugin": "0.5.5",
    "@svgr/webpack": "6.2.1",
    "@types/react": "18.0.9",
    "@types/react-dom": "18.0.3",
    "@typescript-eslint/eslint-plugin": "5.22.0",
    "@typescript-eslint/parser": "5.22.0",
    "babel-loader": "8.2.5",
    "copy-webpack-plugin": "10.2.4",
    "cross-env": "7.0.3",
    "css-loader": "6.7.1",
    "css-minimizer-webpack-plugin": "3.4.1",
    "eslint": "8.14.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-typescript": "17.0.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-import-resolver-typescript": "2.7.1",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsx-a11y": "6.5.1",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-react": "7.29.4",
    "eslint-plugin-react-hooks": "4.5.0",
    "eslint-plugin-simple-import-sort": "7.0.0",
    "html-webpack-plugin": "5.5.0",
    "husky": "7.0.4",
    "mini-css-extract-plugin": "2.6.0",
    "prettier": "2.6.2",
    "prettier-plugin-package": "1.3.0",
    "react-refresh": "0.13.0",
    "style-loader": "3.3.1",
    "typescript": "4.6.4",
    "typescript-plugin-css-modules": "3.4.0",
    "webpack": "5.72.0",
    "webpack-bundle-analyzer": "4.5.0",
    "webpack-cli": "4.9.2",
    "webpack-dev-server": "4.8.1",
    "webpack-merge": "5.8.0"
  }
}
