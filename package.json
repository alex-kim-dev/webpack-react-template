{
  "name": "webpack-react-template",
  "version": "1.0.0-alpha.0",
  "private": true,
  "description": "Webpack 5 template for React Typescript projects.",
  "license": "MIT",
  "repository": "git@github.com:alex-kim-dev/webpack-react-template.git",
  "author": "Alex Kim (https://github.com/alex-kim-dev)",
  "homepage": "https://github.com/alex-kim-dev/webpack-react-template#readme",
  "bugs": "https://github.com/alex-kim-dev/webpack-react-template/issues",
  "scripts": {
    "analyze": "npm run build -- --analyze",
    "build": "cross-env NODE_ENV=production webpack",
    "format": "prettier --check \"**/*.{html,json,yml,md}\"",
    "lint:styles": "stylelint --cache --ignore-path .gitignore \"**/*.{css,scss}\"",
    "lint:ts": "eslint --cache --ignore-path .gitignore --ext .ts,.tsx .",
    "prepare": "husky install",
    "serve": "webpack serve --history-api-fallback --static-directory ./dist",
    "start": "webpack serve --hot",
    "test": "jest --watch",
    "test:ci": "jest --ci",
    "test:coverage": "jest --coverage",
    "typecheck": "tsc",
    "update-caniuse": "npx browserslist --update-db"
  },
  "dependencies": {
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@babel/core": "7.23.2",
    "@babel/preset-env": "7.23.2",
    "@babel/preset-react": "7.22.15",
    "@babel/preset-typescript": "7.23.2",
    "@commitlint/cli": "17.7.2",
    "@commitlint/config-conventional": "17.7.0",
    "@pmmmwh/react-refresh-webpack-plugin": "0.5.11",
    "@svgr/webpack": "8.1.0",
    "@testing-library/jest-dom": "6.1.4",
    "@testing-library/react": "14.0.0",
    "@testing-library/user-event": "14.5.1",
    "@types/jest": "29.5.5",
    "@types/node": "20.8.4",
    "@types/react": "18.2.28",
    "@types/react-dom": "18.2.13",
    "@typescript-eslint/eslint-plugin": "6.7.5",
    "@typescript-eslint/parser": "6.7.5",
    "babel-loader": "9.1.3",
    "copy-webpack-plugin": "11.0.0",
    "cross-env": "7.0.3",
    "css-loader": "6.8.1",
    "css-minimizer-webpack-plugin": "5.0.1",
    "eslint": "8.51.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-typescript": "17.1.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-import-resolver-typescript": "3.6.1",
    "eslint-plugin-import": "2.28.1",
    "eslint-plugin-jest": "27.4.2",
    "eslint-plugin-jest-dom": "5.1.0",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-prettier": "5.0.1",
    "eslint-plugin-react": "7.33.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-simple-import-sort": "10.0.0",
    "eslint-plugin-testing-library": "6.1.0",
    "html-webpack-plugin": "5.5.3",
    "husky": "8.0.3",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "lint-staged": "14.0.1",
    "mini-css-extract-plugin": "2.7.6",
    "prettier": "3.0.3",
    "prettier-plugin-package": "1.3.1",
    "react-refresh": "0.14.0",
    "sass": "1.69.3",
    "sass-loader": "13.3.2",
    "style-loader": "3.3.3",
    "stylelint": "15.10.3",
    "stylelint-config-idiomatic-order": "9.0.0",
    "stylelint-config-standard-scss": "11.0.0",
    "stylelint-order": "6.0.3",
    "stylelint-prettier": "4.0.2",
    "tsc-files": "1.1.4",
    "type-fest": "4.4.0",
    "typescript": "5.2.2",
    "typescript-plugin-css-modules": "5.0.1",
    "webpack": "5.88.2",
    "webpack-bundle-analyzer": "4.9.1",
    "webpack-cli": "5.1.4",
    "webpack-dev-server": "4.15.1",
    "webpack-merge": "5.9.0"
  },
  "overrides": {
    "prettier-plugin-package@1.3.1": {
      "prettier": "^3.0.0"
    },
    "stylelint-order@5.0.0": {
      "stylelint": "^15.0.0"
    }
  }
}
