{
  "name": "webpack-react-template",
  "version": "1.0.9",
  "private": true,
  "description": "Webpack 5 template for React projects.",
  "license": "MIT",
  "repository": "git@github.com:alex-kim-dev/webpack-react-template.git",
  "author": "Alex Kim <45559664+alex-kim-dev@users.noreply.github.com>",
  "bugs": "https://github.com/alex-kim-dev/webpack-react-template/issues",
  "scripts": {
    "analyze": "npm run build -- --analyze",
    "build": "webpack --config ./config/webpack.prod.js",
    "format": "prettier --check \"**/*.{json,yml,yaml,md}\"",
    "format:fix": "prettier --write \"**/*.{json,yml,yaml,md}\"",
    "lint": "eslint --ignore-path .gitignore --ext js,jsx .",
    "lint:fix": "eslint --ignore-path .gitignore --fix --ext js,jsx .",
    "serve": "lite-server --baseDir ./dist",
    "start": "webpack serve --config ./config/webpack.dev.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "update-caniuse": "npx browserslist --update-db"
  },
  "keywords": [
    "webpack 5",
    "template",
    "react"
  ],
  "dependencies": {
    "react": "^18.1.0",
    "react-dom": "^18.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.17.9",
    "@babel/eslint-parser": "^7.17.0",
    "@babel/plugin-proposal-class-properties": "^7.16.7",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.5",
    "@svgr/webpack": "^6.2.1",
    "babel-loader": "^8.2.5",
    "copy-webpack-plugin": "^10.2.4",
    "css-loader": "^6.7.1",
    "css-minimizer-webpack-plugin": "^3.4.1",
    "eslint": "^8.14.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-webpack": "^0.13.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.5.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "html-webpack-plugin": "^5.5.0",
    "lite-server": "^2.6.1",
    "mini-css-extract-plugin": "^2.6.0",
    "prettier": "^2.6.2",
    "prettier-plugin-package": "^1.3.0",
    "react-refresh": "^0.13.0",
    "style-loader": "^3.3.1",
    "webpack": "^5.72.0",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.8.1",
    "webpack-merge": "^5.8.0"
  }
}
