{
  "name": "webpack-react-template",
  "version": "0.2.8",
  "description": "Webpack 5 template for React projects.",
  "license": "MIT",
  "repository": "git@github.com:Alex-K1m/webpack-react-template.git",
  "author": "Alex Kim <45559664+Alex-K1m@users.noreply.github.com>",
  "bugs": "https://github.com/Alex-K1m/webpack-react-template/issues",
  "main": "index.js",
  "scripts": {
    "analyze": "npm run build -- --analyze",
    "build": "cross-env NODE_ENV=production webpack --config ./config/webpack.prod.js",
    "format": "prettier --check \"**/*.{json,yml,yaml,md}\"",
    "format:fix": "prettier --write \"**/*.{json,yml,yaml,md}\"",
    "lint": "eslint --ignore-path .gitignore --ext js,jsx .",
    "lint:fix": "eslint --ignore-path .gitignore --fix --ext js,jsx .",
    "start": "cross-env NODE_ENV=development webpack serve --config ./config/webpack.dev.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "update-caniuse": "npx browserslist --update-db"
  },
  "keywords": [
    "webpack 5",
    "template",
    "react"
  ],
  "dependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/preset-env": "^7.14.7",
    "@babel/preset-react": "^7.14.5",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.4.3",
    "@svgr/webpack": "^5.5.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^9.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^5.2.6",
    "css-minimizer-webpack-plugin": "^3.0.1",
    "eslint": "^7.29.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-webpack": "^0.13.1",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "html-webpack-plugin": "^5.3.2",
    "mini-css-extract-plugin": "^1.6.0",
    "prettier": "^2.3.1",
    "prettier-plugin-package": "^1.3.0",
    "react-refresh": "^0.10.0",
    "style-loader": "^3.0.0",
    "webpack": "^5.40.0",
    "webpack-bundle-analyzer": "^4.4.2",
    "webpack-cli": "^4.7.2",
    "webpack-dev-server": "^3.11.2",
    "webpack-merge": "^5.8.0"
  }
}
