{
  "name": "webpack-react-template",
  "version": "1.0.0-alpha.0",
  "private": true,
  "description": "Webpack 5 template for React Typescript projects.",
  "license": "MIT",
  "repository": "git@github.com:alex-kim-dev/webpack-react-template.git",
  "author": "Alex Kim (https://github.com/alex-kim-dev)",
  "homepage": "https://github.com/alex-kim-dev/webpack-react-template#readme",
  "bugs": "https://github.com/alex-kim-dev/webpack-react-template/issues",
  "scripts": {
    "analyze": "npm run build -- --analyze",
    "build": "cross-env NODE_ENV=production webpack",
    "format": "prettier --check --ignore-path .gitignore \"**/*.{html,json,yml,md}\"",
    "lint:styles": "stylelint --cache --ignore-path .gitignore \"**/*.{css,scss}\"",
    "lint:ts": "eslint --cache --ignore-path .gitignore --ext .ts,.tsx .",
    "prepare": "husky install",
    "serve": "webpack serve --history-api-fallback --static-directory ./dist",
    "start": "webpack serve --hot",
    "test": "jest --watch",
    "test:ci": "jest --ci",
    "test:coverage": "jest --coverage",
    "typecheck": "tsc",
    "update-caniuse": "npx browserslist --update-db"
  },
  "dependencies": {
    "@types/node": "18.8.5",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@babel/core": "7.19.3",
    "@babel/preset-env": "7.19.4",
    "@babel/preset-react": "7.18.6",
    "@babel/preset-typescript": "7.18.6",
    "@commitlint/cli": "17.1.2",
    "@commitlint/config-conventional": "17.1.0",
    "@pmmmwh/react-refresh-webpack-plugin": "0.5.8",
    "@svgr/webpack": "6.4.0",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "13.4.0",
    "@testing-library/user-event": "14.4.3",
    "@types/jest": "29.1.2",
    "@types/react": "18.0.21",
    "@types/react-dom": "18.0.6",
    "@types/testing-library__jest-dom": "5.14.5",
    "@typescript-eslint/eslint-plugin": "5.40.0",
    "@typescript-eslint/parser": "5.40.0",
    "babel-loader": "8.2.5",
    "copy-webpack-plugin": "11.0.0",
    "cross-env": "7.0.3",
    "css-loader": "6.7.1",
    "css-minimizer-webpack-plugin": "4.2.2",
    "eslint": "8.25.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-typescript": "17.0.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-import-resolver-typescript": "3.5.1",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jest": "27.1.1",
    "eslint-plugin-jest-dom": "4.0.2",
    "eslint-plugin-jsx-a11y": "6.6.1",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "7.31.10",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-simple-import-sort": "8.0.0",
    "eslint-plugin-testing-library": "5.7.2",
    "html-webpack-plugin": "5.5.0",
    "husky": "8.0.1",
    "jest": "29.1.2",
    "jest-environment-jsdom": "29.1.2",
    "lint-staged": "13.0.3",
    "mini-css-extract-plugin": "2.6.1",
    "prettier": "2.7.1",
    "prettier-plugin-package": "1.3.0",
    "react-refresh": "0.14.0",
    "sass": "1.55.0",
    "sass-loader": "13.1.0",
    "style-loader": "3.3.1",
    "stylelint": "14.13.0",
    "stylelint-config-idiomatic-order": "9.0.0",
    "stylelint-config-prettier": "9.0.3",
    "stylelint-config-standard-scss": "5.0.0",
    "stylelint-order": "5.0.0",
    "stylelint-prettier": "2.0.0",
    "tsc-files": "1.1.3",
    "typescript": "4.8.4",
    "typescript-plugin-css-modules": "3.4.0",
    "webpack": "5.74.0",
    "webpack-bundle-analyzer": "4.6.1",
    "webpack-cli": "4.10.0",
    "webpack-dev-server": "4.11.1",
    "webpack-merge": "5.8.0"
  }
}
